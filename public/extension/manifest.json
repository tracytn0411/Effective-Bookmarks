{
  "name": "Effective Bookmarks",
  "version": "1.0",
  "description": "effective bookmark",
  "permissions": [
    "storage", 
    "declarativeContent",
    "tabs",
    "<all_urls>",
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [ "<all_url>" ],
      "js": ["jquery-3.4.1.min.js", "popup.js"]
    }
  ],
  "manifest_version": 2,

  "browser_action": {
    "default_title": "URL CATCHER",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/bookmarkicon16.png",
      "32": "images/bookmarkicon32.png",
      "48": "images/bookmarkicon48.png",
      "128": "images/bookmarkicon128.png"
    }
  },
  "icons": {
    "16": "images/bookmarkicon16.png",
    "32": "images/bookmarkicon32.png",
    "48": "images/bookmarkicon48.png",
    "128": "images/bookmarkicon128.png"
  }
}